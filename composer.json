{
    "name": "c975l/pageedit-bundle",
    "type": "symfony-bundle",
    "description": "Create, modify and manage web pages",
    "keywords": ["symfony", "bundle", "pages", "modify"],
    "homepage": "https://github.com/975L/PageEditBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Laurent Marquet",
            "email": "laurent.marquet@laposte.net",
            "homepage": "https://github.com/LaurentMarquet",
            "role": "Developer"
        },
        {
            "name": "975L Community",
            "homepage": "https://github.com/975L"
        }
    ],
    "require": {
        "php": "^7",
        "c975l/config-bundle": "^2",
        "c975l/includelibrary-bundle": "^1",
        "c975l/services-bundle": "^1",
        "c975l/toolbar-bundle": "^1",
        "cocur/slugify": "^3",
        "h4cc/wkhtmltopdf-amd64": "^0",
        "knplabs/knp-paginator-bundle": "^4",
        "knplabs/knp-snappy-bundle": "^1",
        "symfony/form": "*",
        "symfony/security": "*",
        "symfony/templating": "*",
        "symfony/translation": "*",
        "twig/extensions": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "rector/rector": "^0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "c975L\\PageEditBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "support": {
        "email": "contact@975l.com",
        "source": "https://github.com/975L/PageEditBundle",
        "issues": "https://github.com/975L/PageEditBundle/issues"
    }
}
